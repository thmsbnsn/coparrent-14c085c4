# Frontend (Vite) env vars
# Copy to `.env.local` for local dev. Never commit real secrets.

# Supabase
VITE_SUPABASE_URL=
VITE_SUPABASE_PUBLISHABLE_KEY=

# Stripe (frontend)
VITE_STRIPE_PUBLISHABLE_KEY=

# Optional: Stripe Power tier mapping (recommended once you create Stripe products/prices)
VITE_STRIPE_POWER_PRICE_ID=
VITE_STRIPE_POWER_PRODUCT_ID=

# Optional: test mode mappings (leave blank in prod)
VITE_STRIPE_TEST_POWER_PRICE_ID=
VITE_STRIPE_TEST_POWER_PRODUCT_ID=

